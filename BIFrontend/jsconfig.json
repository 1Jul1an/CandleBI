{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext"
  },
  "exclude": [
    "node_modules",
    ".svelte-kit",
    "build",
    "dist",
    "e2e",
    "static",
    "**/__snapshots__"
  ]
}
